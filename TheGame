#=================================================================Enemies===================================================================================#
class Enemy():
    def __init__(self, name, hp, damage):
        self.name = name
        self.hp = hp
        self.damage = damage
 
    def is_alive(self):
        return self.hp > 0


class GiantSpider(Enemy):
    def __init__(self):
        super().__init__(name="Giant Spider", hp=7, damage=2)

class Goblin(Enemy):
    def __init__(self):
        super().__init__(name="Ogre", hp=15, damage=7) 
 
class Ogre(Enemy):
    def __init__(self):
        super().__init__(name="Ogre", hp=30, damage=7)
