def GoHome():
     print('You decide to go home and rest')
     print('----------------------------------------------')
     print('')
     time.sleep(.4)
     resthealth = random.randint(0, 15)
     hero.health += resthealth
     print("You gained " + str(resthealth) + " health from resting!")
     print(hero.health)
     GameStart()
 
 
 
