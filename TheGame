def StoreFront():
         print('a. Health Management   b. Armoury   c. Weapons   d. Sell')
         storeCat = input()
        
         if storeCat in ('a', 'A'):
              HealthManagement1()
         elif storeCat in ('B', 'b'):
              Armoury1()
         elif storeCat in ('C', 'c'):
              Weapons1()
         elif storeCat in ('D', 'd'):
              Sell()



