#=================================================================Fight=====================================================================================#
def Fighting():
    def attSlash():
        print("place holder Slash")
        print("Will need to return to fighting sequence after called so enemy can attack.")
        time.sleep(.2)
        Fighting()
    def attBludgeon():
        print("place holder Bld")
        print("Will need to return to fighting sequence after called so enemy can attack.")
        time.sleep(.2)
        Fighting()
    def attStab():
        print("place holder Stab")
        print("Will need to return to fighting sequence after called so enemy can attack.")
        time.sleep(.2)
        Fighting()
    def attBowArrow():
        print("place holder B&A")
        print("Will need to return to fighting sequence after called so enemy can attack.")
        time.sleep(.2)
        Fighting()






    print("-------------------------------------------------------------------")
    print("How do you want to attack?")
    print("a.   Slash")
    print("b.   Bludgeon")
    print("c.   Stab")
    print("d.   Bow & Arrow")
    print("e.   Exit")
    attack = input()
    
    if attack in ("a", "A", "Slash", "slash",):
        print("place holder for slashing dmg to enemy")
        attSlash()
    elif attack in ("b", "B", "Bludgeon", "bludgeon",):
        print("place holder for slashing dmg to enemy")
        attBludgeon()
    elif attack in ("c", "C", "Stab", "stab",):
        print("place holder for slashing dmg to enemy")
        attStab()
    elif attack in ("d", "D", "Bow & Arrow", "bow and arrow",):
        print("place holder for slashing dmg to enemy")
        attBowArrow()
    elif attack in ("e", "E"):
        GameStart()
    else:
        print("That is not a propper attack pattern. Try again.")
        time.sleep(.5)
        Fighting()

    
    







