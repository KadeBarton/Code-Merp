class LivingThing():
        def __init__(self, name, health, inventory, gold):
                self.name = name
                self.health = health
                self.inventory = inventory
                self.gold = gold
        def TakeDamage(self, dmgAmount):
                self.health = self.health - dmgAmount
                if self.health == 0:
                        print(self.name + ' is dead!')
        def loseGold(self, loseGoldAmount):
                self.gold = self.gold - loseGoldAmount
                if self.gold <= 0:
                        print('You have no money.')
        def gainGold(self, gainGoldAmount):
                self.gold = self.gold + gainGoldAmount
                if self.gold >= 0:
                        print('You have ' + gold + 'g.')

hero = LivingThing('HERONAME', 20, {}, 0)

