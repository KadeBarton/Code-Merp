def Die():
     print('Are you really sure you want to do this? All progress will be lost and you will have to start over.. (yes/no)')
     death = input()

     if death == 'yes':
             print('')
             print('')
             print('')
             print('')
             print('Farewell cruel world! I cannot live on here any longer!')
             print('')
             print('')
             print('')
             print('')
             print('xxxx                      xxxx')
             print('  xxxx                  xxxx')
             print('    xxxx              xxxx')
             print('      xxxx          xxxx')
             print('        xxxx      xxxx')
             print('          xxxx  xxxx')
             print(' Game      xxxxxxxx')
             print('           xxxxxxxx      Over  ')
             print('          xxxx  xxxx')
             print('        xxxx      xxxx')
             print('      xxxx          xxxx')
             print('    xxxx              xxxx')
             print('  xxxx                  xxxx')
             print('xxxx                      xxxx')
             
             time.sleep(3)
             Credits()
     else:
             print('Good call, now get back out there and adventure!')
             time.sleep(1.5)
             GameStart()
 

